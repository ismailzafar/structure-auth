  "scripts": {
    "compile": "node_modules/.bin/babel src -d dist -D -s",
    "prepublish": "npm run compile",
    "start-reqlite": "node_modules/forever/bin/forever start node_modules/reqlite/lib/node.js --port-offset 1 -s",
